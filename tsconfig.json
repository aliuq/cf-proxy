{
  "compilerOptions": {
    "target": "es2021",
    "lib": [
      "es2021",
      "dom"
    ],
    "jsx": "react",
    "module": "es2022",
    "moduleResolution": "node",
    "types": [
      "@cloudflare/workers-types",
      "node"
    ],
    "resolveJsonModule": true,
    "allowJs": true,
    "checkJs": false,
    "noEmit": true,
    "isolatedModules": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "exclude": [
    "**/node_modules",
    "**/test"
  ]
}
